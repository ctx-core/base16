{
	"name": "@ctx-core/base16",
	"version": "7.0.7",
	"description": "ctx-core base16",
	"keywords": [
		"ctx-core",
		"base16"
	],
	"homepage": "https://github.com/ctx-core/base16#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/base16/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/base16.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"require = require('esm')(module)\nmodule.exports = require('./index.js')\" > index.cjs",
		"exec": "$@"
	},
	"devDependencies": {
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"dependencies": {
		"esm": "^3.2.25"
	}
}
